<sdf version='1.7'>
  <model name='quadcopter'>
    <link name='base_link'>
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.03</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.03</iyy>
          <iyz>0</iyz>
          <izz>0.06</izz>
        </inertia>
      </inertial>

      <collision name='base_collision'>
        <geometry>
          <box>
            <size>0.4 0.4 0.05</size>
          </box>
        </geometry>
      </collision>

      <visual name='base_visual'>
        <geometry>
          <box>
            <size>0.2 0.2 0.05</size>
          </box>
        </geometry>
        <material>
          <ambient>0.1 0.1 0.1 1</ambient>
          <diffuse>0.2 0.2 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>

      <visual name='arm_fr_visual'>
        <pose>0.15 -0.15 0 0 0 -0.785</pose>
        <geometry>
          <box>
            <size>0.3 0.04 0.03</size>
          </box>
        </geometry>
        <material>
          <ambient>0.8 0.2 0.2 1</ambient>
          <diffuse>0.8 0.2 0.2 1</diffuse>
        </material>
      </visual>

      <visual name='arm_fl_visual'>
        <pose>0.15 0.15 0 0 0 0.785</pose>
        <geometry>
          <box>
            <size>0.3 0.04 0.03</size>
          </box>
        </geometry>
        <material>
          <ambient>0.2 0.8 0.2 1</ambient>
          <diffuse>0.2 0.8 0.2 1</diffuse>
        </material>
      </visual>

      <visual name='arm_rl_visual'>
        <pose>-0.15 0.15 0 0 0 -0.785</pose>
        <geometry>
          <box>
            <size>0.3 0.04 0.03</size>
          </box>
        </geometry>
         <material>
          <ambient>0.2 0.2 0.8 1</ambient>
          <diffuse>0.2 0.2 0.8 1</diffuse>
        </material>
      </visual>

      <visual name='arm_rr_visual'>
        <pose>-0.15 -0.15 0 0 0 0.785</pose>
        <geometry>
          <box>
            <size>0.3 0.04 0.03</size>
          </box>
        </geometry>
        <material>
          <ambient>0.8 0.8 0.2 1</ambient>
          <diffuse>0.8 0.8 0.2 1</diffuse>
        </material>
      </visual>
      
      <sensor name="imu_sensor" type="imu">
        <always_on>1</always_on>
        <update_rate>100</update_rate>
        <visualize>true</visualize>
        <topic>imu</topic>
        <pose>0 0 0 0 0 0</pose>
        <imu>
          <angular_velocity>
            <x>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </x>
            <y>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </y>
            <z>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </z>
          </angular_velocity>
          <linear_acceleration>
            <x>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </x>
            <y>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </y>
            <z>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </z>
          </linear_acceleration>
        </imu>
      </sensor>

      <sensor name="magnetometer_sensor" type="magnetometer">
        <always_on>1</always_on>
        <update_rate>50</update_rate>
        <visualize>true</visualize>
        <topic>magnetometer</topic>
        <pose>0 0 0 0 0 0</pose>
        <magnetometer>
          <x>
            <noise type='gaussian'>
              <stddev>.003</stddev>
            </noise>
          </x>
          <y>
            <noise type='gaussian'>
              <stddev>.003</stddev>
            </noise>
          </y>
          <z>
            <noise type='gaussian'>
              <stddev>.003</stddev>
            </noise>
          </z>
        </magnetometer>
      </sensor>

      <sensor name="gps_sensor" type="navsat">
        <always_on>1</always_on>
        <update_rate>10</update_rate>
        <visualize>true</visualize>
        <topic>gps</topic>
        <pose>0 0 0 0 0 0</pose>
        <navsat>
          <position_sensing>
            <horizontal>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </horizontal>
            <vertical>
              <noise type='gaussian'>
                <stddev>0</stddev>
              </noise>
            </vertical>
          </position_sensing>
        </navsat>
      </sensor>
    </link>

    <link name="prop_fr_link">
      <pose>0.212 -0.212 0.02 0 0 0</pose>
      <inertial><mass>0.01</mass><inertia><ixx>0.0001</ixx><iyy>0.0001</iyy><izz>0.0002</izz></inertia></inertial>
      <visual name='prop_fr_visual'>
        <geometry><cylinder><radius>0.12</radius><length>0.01</length></cylinder></geometry>
        <material><ambient>0.9 0.9 0.9 0.4</ambient><diffuse>1 1 1 0.4</diffuse></material>
      </visual>
    </link>
    <link name="prop_fl_link">
      <pose>0.212 0.212 0.02 0 0 0</pose>
      <inertial><mass>0.01</mass><inertia><ixx>0.0001</ixx><iyy>0.0001</iyy><izz>0.0002</izz></inertia></inertial>
      <visual name='prop_fl_visual'>
        <geometry><cylinder><radius>0.12</radius><length>0.01</length></cylinder></geometry>
        <material><ambient>0.9 0.9 0.9 0.4</ambient><diffuse>1 1 1 0.4</diffuse></material>
      </visual>
    </link>
    <link name="prop_rl_link">
      <pose>-0.212 0.212 0.02 0 0 0</pose>
      <inertial><mass>0.01</mass><inertia><ixx>0.0001</ixx><iyy>0.0001</iyy><izz>0.0002</izz></inertia></inertial>
      <visual name='prop_rl_visual'>
        <geometry><cylinder><radius>0.12</radius><length>0.01</length></cylinder></geometry>
        <material><ambient>0.9 0.9 0.9 0.4</ambient><diffuse>1 1 1 0.4</diffuse></material>
      </visual>
    </link>
    <link name="prop_rr_link">
      <pose>-0.212 -0.212 0.02 0 0 0</pose>
      <inertial><mass>0.01</mass><inertia><ixx>0.0001</ixx><iyy>0.0001</iyy><izz>0.0002</izz></inertia></inertial>
      <visual name='prop_rr_visual'>
        <geometry><cylinder><radius>0.12</radius><length>0.01</length></cylinder></geometry>
        <material><ambient>0.9 0.9 0.9 0.4</ambient><diffuse>1 1 1 0.4</diffuse></material>
      </visual>
    </link>

    <joint name="prop_fr_joint" type="revolute">
      <pose relative_to="prop_fr_link"/>
      <parent>base_link</parent>
      <child>prop_fr_link</child>
      <axis><xyz>0 0 1</xyz></axis>
    </joint>
    <joint name="prop_fl_joint" type="revolute">
      <pose relative_to="prop_fl_link"/>
      <parent>base_link</parent>
      <child>prop_fl_link</child>
      <axis><xyz>0 0 1</xyz></axis>
    </joint>
    <joint name="prop_rl_joint" type="revolute">
      <pose relative_to="prop_rl_link"/>
      <parent>base_link</parent>
      <child>prop_rl_link</child>
      <axis><xyz>0 0 1</xyz></axis>
    </joint>
    <joint name="prop_rr_joint" type="revolute">
      <pose relative_to="prop_rr_link"/>
      <parent>base_link</parent>
      <child>prop_rr_link</child>
      <axis><xyz>0 0 1</xyz></axis>
    </joint>

    <plugin filename="gz-sim-imu-system"
        name="gz::sim::systems::Imu">
    </plugin>
    
    <plugin filename="gz-sim-magnetometer-system"
      name="gz::sim::systems::Magnetometer">
    </plugin>
    
    <plugin filename="gz-sim-navsat-system"
      name="gz::sim::systems::NavSat">
    </plugin>
    
    
    <plugin
      filename="gz-sim-multicopter-motor-model-system"
      name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>quadcopter</robotNamespace>
      <jointName>prop_fr_joint</jointName>
      <linkName>prop_fr_link</linkName>
      <turningDirection>ccw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>800.0</maxRotVelocity>
      <motorConstant>1.0258e-05</motorConstant>
      <momentConstant>0.016</momentConstant>
      <commandSubTopic>cmd_vel</commandSubTopic>
      <actuator_number>1</actuator_number>
      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>motor_speed/1</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
      <motorType>velocity</motorType>
    </plugin>
    <plugin
      filename="gz-sim-multicopter-motor-model-system"
      name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>quadcopter</robotNamespace>
      <jointName>prop_fl_joint</jointName>
      <linkName>prop_fl_link</linkName>
      <turningDirection>cw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>800.0</maxRotVelocity>
      <motorConstant>1.0258e-05</motorConstant>
      <momentConstant>0.016</momentConstant>
      <commandSubTopic>cmd_vel</commandSubTopic>
      <actuator_number>3</actuator_number>
      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>motor_speed/3</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
      <motorType>velocity</motorType>
    </plugin>
    <plugin
      filename="gz-sim-multicopter-motor-model-system"
      name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>quadcopter</robotNamespace>
      <jointName>prop_rl_joint</jointName>
      <linkName>prop_rl_link</linkName>
      <turningDirection>ccw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>800.0</maxRotVelocity>
      <motorConstant>1.0258e-05</motorConstant>
      <momentConstant>0.016</momentConstant>
      <commandSubTopic>cmd_vel</commandSubTopic>
      <actuator_number>2</actuator_number>
      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>motor_speed/2</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
      <motorType>velocity</motorType>
    </plugin>
    <plugin
      filename="gz-sim-multicopter-motor-model-system"
      name="gz::sim::systems::MulticopterMotorModel">
      <robotNamespace>quadcopter</robotNamespace>
      <jointName>prop_rr_joint</jointName>
      <linkName>prop_rr_link</linkName>
      <turningDirection>cw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>800.0</maxRotVelocity>
      <motorConstant>1.0258e-05</motorConstant>
      <momentConstant>0.016</momentConstant>
      <commandSubTopic>cmd_vel</commandSubTopic>
      <actuator_number>0</actuator_number>
      <rotorDragCoefficient>8.06428e-05</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>motor_speed/0</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
      <motorType>velocity</motorType>
    </plugin>
    <plugin
        filename="gz-sim-multicopter-control-system"
        name="gz::sim::systems::MulticopterVelocityControl">
        <robotNamespace>quadcopter</robotNamespace>
        <commandSubTopic>vel_cmd</commandSubTopic>
        <enableSubTopic>enable</enableSubTopic>
        <comLinkName>base_link</comLinkName>
        <velocityGain>2.7 2.7 2.7</velocityGain>
        <attitudeGain>2 3 0.15</attitudeGain>
        <angularRateGain>0.4 0.52 0.18</angularRateGain>
        <maximumLinearAcceleration>2 2 2</maximumLinearAcceleration>

        <rotorConfiguration>
          <rotor>
            <jointName>prop_rr_joint</jointName>
            <forceConstant>1.0258e-05</forceConstant>
            <momentConstant>0.016</momentConstant>
            <direction>-1</direction>
          </rotor>
          <rotor>
            <jointName>prop_fr_joint</jointName>
            <forceConstant>1.0258e-05</forceConstant>
            <momentConstant>0.016</momentConstant>
            <direction>1</direction>
          </rotor>
          <rotor>
            <jointName>prop_rl_joint</jointName>
            <forceConstant>1.0258e-05</forceConstant>
            <momentConstant>0.016</momentConstant>
            <direction>1</direction>
          </rotor>
          <rotor>
            <jointName>prop_fl_joint</jointName>
            <forceConstant>1.0258e-05</forceConstant>
            <momentConstant>0.016</momentConstant>
            <direction>-1</direction>
          </rotor>
        </rotorConfiguration>
      </plugin>
  </model>
</sdf>